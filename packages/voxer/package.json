  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^9.3.0",
    "cornsol": "^1.1.4",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "latest",
    "glob": "^8.0.3",
    "prettier": "^2.6.2",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "webpack": "^5.75.0",
    "webpack-preprocessor-loader": "^1.2.0"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/prettier": "^2.6.1",
    "mousetrap": "^1.6.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  }
