{
  "extends": "../.voxer/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "lib": ["esnext", "dom"],
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "#imports/*": ["../.voxer/dist/src/*"]
    }
  },
  "include": [
    "./**/*.ts",
    "./**/*.d.ts",
    "./**/*.tsx",
    "./**/*.vue",
    "../.voxer/api.d.ts"
  ]
}
